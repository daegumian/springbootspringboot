package com.simple.basic.command;

import javax.validation.constraints.NotBlank;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Builder
public class MemoVO {
	
	@NotBlank
	private String memo;
	
	@NotBlank
	private String phone;
	
	@NotBlank
	private String pw;

}
